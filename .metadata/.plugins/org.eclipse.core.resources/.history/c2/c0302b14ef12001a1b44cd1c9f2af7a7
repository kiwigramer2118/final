package model;

import processing.core.PApplet;
import processing.core.PImage;

public class Omero implements Runnable{
	

	private PImage img;
	private int posX;
	private int posY;
	private PApplet appp;

	public Omero(PApplet app) {
		appp = app;
	img=appp.loadImage("./img/homero_simpson.png");
	posX=(int) appp.random(150,750);
	posY=(int) appp.random(50,750);
	
		
		
		
	}

	@Override
	public void run() {
		// TODO Auto-generated method stub
		
	}

}
