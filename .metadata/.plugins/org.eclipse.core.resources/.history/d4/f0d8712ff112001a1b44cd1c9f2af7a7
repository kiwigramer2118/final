package model;

import processing.core.PApplet;
import view.Main;

public class Logica extends Main{
private int click;

     Thread hom;

	private PApplet app;
	public Logica(PApplet app){
		
		this.app=app;

		Thread hom =new Thread(new Omero(this));
		
	}
	
	public void pintar() {
		app.ellipse(100, 100, 100, 100);
		
		hom.start();
		
		
	}
}
