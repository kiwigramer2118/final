package model;

import processing.core.PApplet;
import view.Main;

public class Logica extends Main{
private int click;
Omero om;
private PApplet app;

  
	public Logica(PApplet app){
		
		this.app=app;
		Omero om =new Omero(app);
	    Thread hom =new Thread(new Omero(this));
		
		hom.start();
	

	
		
	}
	
	public void pintar() {
		
		om.pintar();
		
		
		
		
	}
}
