package model;

import processing.core.PApplet;
import processing.core.PImage;

public class Omero {
	
	private PApplet app;
	private PImage img;
	private int posX;
	private int posY;

	public Omero(PApplet app) {
		this.app = app;
	img=app.loadImage("img/homero_simpson");
	this.posX=(int) app.random(150,750);
	app.image(img,this.posX,this.posY,100,100);
		
		
		
	}

}
